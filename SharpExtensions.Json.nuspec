<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SharpExtensions.Json</id>
    <version>0.1.2</version>
    <authors>TheGreatCO</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>A collection of extension methods for Json to make things a bit easier.</description>
    <releaseNotes>Added some numeric extensions, refactored the location of some extensions. Future versions will all be released in lockstep to keep consistent.</releaseNotes>
    <projectUrl>https://www.thegreatco.com/projects/sharpextensions/</projectUrl>
    <language>en-US</language>
    <dependencies>
      <group targetFramework=".NETFramework4.0">
        <dependency id="Newtonsoft.Json" version="5.0.8" />
      </group>
      <group targetFramework=".NETFramework4.5">
        <dependency id="Newtonsoft.Json" version="5.0.8" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="NuGet Files\SharpExtensions45.Json\*.pp" target="content\net45\Extensions" />
    <file src="NuGet Files\SharpExtensions40.Json\*.pp" target="content\net40\Extensions" />
  </files>
</package>