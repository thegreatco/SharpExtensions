{
  "supports": {},
  "dependencies": {
    "Microsoft.NETCore.Portable.Compatibility": "1.0.1",
    "NETStandard.Library": "1.6.0",
    "Newtonsoft.Json": "9.0.1"
  },
  "frameworks": {
    "netstandard1.1": {}
  },
  "description": "A collection of extension methods to make Json things a bit easier.",
  "authors": [ "TheGreatCO" ],
  "version": "0.1.8",
  "name": "SharpExtensions.Json",
  "language": "en-US",
  "buildOptions": {
    "define": [ "NETDX" ],
    "platform": "anycpu",
    "debugType": "portable",
    "xmlDoc": true
  },
  "packOptions": {
    "releaseNotes": "Removed support for .NET 40, this package now targets .NETStandard 1.1 supporting .NET 4.5 and newer.",
    "projectUrl": "https://github.com/thegreatco/SharpExtensions",
    "licenseUrl": "https://github.com/thegreatco/SharpExtensions/blob/master/LICENSE.TXT",
    "requireLicenseAcceptance": true,
    "repository": {
      "type": "git",
      "url": "https://github.com/thegreatco/SharpExtensions"
    }
  },
  "scripts": {
    "postcompile": [ "dotnet pack --no-build --configuration %compile:Configuration%" ]
  }
}